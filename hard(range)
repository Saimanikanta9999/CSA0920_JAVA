 import java.util.ArrayList;
import java.util.List;

public class NumberRange {
    public static void main(String[] args) {
        int start = 1; // Start of the range
        int end = 10; // End of the range

        List<Integer> numbersInRange = generateRange(start, end);

        System.out.println("Numbers in the range from " + start + " to " + end + ":");
        System.out.println(numbersInRange);
    }

    public static List<Integer> generateRange(int start, int end) {
        List<Integer> range = new ArrayList<>();
        for (int i = start; i <= end; i++) {
            range.add(i);
        }
        return range;
    }
}
